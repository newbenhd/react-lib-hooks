    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.4"
  },
  "files": [
    "dist/**/*"
  ],
  "bugs": {
    "url": "https://github.com/newbenhd/npm-opensource-template/issues"
  },
  "homepage": "https://github.com/newbenhd/npm-opensource-template#readme",
