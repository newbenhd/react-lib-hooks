{
  "name": "react-lib-hooks",
  "version": "1.0.1",
  "description": "Utility for React: Hooks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+http:s//github.com:newbenhd/npm-opensource-template.git"
  },
  "author": "Benjamin Morrison <newben.hd@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(ts|js)(x)\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "test:dev": "jest --watch",
